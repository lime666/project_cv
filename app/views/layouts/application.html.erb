<!DOCTYPE html>
<html>
  <head>
    <title>ProjectCv</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" href="<%= cvs_path %>">All CVs</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="<%= new_cv_path %>">Create new CV</a>
          </li>
        </ul>
      </nav>
    </div>

    <div class="container main">
      <%= yield %>
    </div>

    <div class="text-center">
      <img src="https://cs12.pikabu.ru/post_img/big/2020/11/21/8/1605962730176845454.png" alt="fdfgd">
    </div>

  </body>
</html>







